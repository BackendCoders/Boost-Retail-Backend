          7,
          8,
          9,
          10,
          11,
          12,
          13,
          14
        ],
        "type": "integer",
        "format": "int32"
      },
      "CategoryDto": {
        "required": [
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "parentId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CategoryLookup": {
        "required": [
          "categorisation",
          "isActive",
          "supplierColumns",
          "supplierFeed",
          "tableName"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "supplierFeed": {
            "$ref": "#/components/schemas/BrandType"
          },
          "tableName": {
            "minLength": 1,
            "type": "string"
